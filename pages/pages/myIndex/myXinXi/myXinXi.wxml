<view id="content">
  <i-row>
    <i-col span="24">
      <i-cell-group>
          <!-- <i-cell title="自考相片" i-class="wb phone" ><image src='../../../images/touxiang.png' class='xinxiImg'></image></i-cell> -->
          <i-cell title="姓名" i-class="wb" value="{{xinxi.name}}"></i-cell>
          <i-cell title="性别" i-class="wb" value="{{xinxi.gender}}"></i-cell>
          <i-cell title="户籍" i-class="wb" value="{{xinxi.homeType}}"></i-cell>
          <i-cell title="政治面貌" i-class="wb" value="{{xinxi.politics}}"></i-cell>
          <i-cell title="民族" i-class="wb" value="{{xinxi.nation}}"></i-cell>
          <i-cell title="职业" i-class="wb" value="{{xinxi.profession}}"></i-cell>
          <i-cell title="籍贯" i-class="wb" value="{{xinxi.address}}"></i-cell>
          <i-cell title="准考证号" i-class="wb" value="{{xinxi.id}}"></i-cell>
          <i-cell title="出生年月" i-class="wb" value="{{birthday}}"></i-cell>
          <i-cell title="证件号码" i-class="wb" value="{{xinxi.certificateNo}}"></i-cell>
          <i-cell title="联系电话" i-class="wb" value="{{xinxi.mphone}}"></i-cell>
      </i-cell-group>
    </i-col>
  </i-row>
  <i-row>
    <i-col span="24">
      <i-button type="primary" bind:click="fanhui">返回</i-button>
    </i-col>
  </i-row>
</view>